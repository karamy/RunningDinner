{
  "appId": "io.runningdinner.starter",
  "appName": "RunningDinner",
  "webDir": "www",
  "npmClient": "npm",
  "plugins": {
    "CapacitorFirebaseAuth": {
      "providers": [
        "google.com",
        "twitter.com",
        "facebook.com",
        "phone"
      ],
      "languageCode": "en",
      "nativeAuth": false,
      "permissions": {
        "google": [
          "profile",
          "https://www.googleapis.com/auth/drive"
        ]
      }
    }
  }
}